{
    "name": "mitsuru793/file-constructor",
    "description": "Make file and directory from php array.",
    "type": "library",
    "homepage": "https://github.com/mitsuru793/php-file-constructor",
    "license": "MIT",
    "keywords": [
        "file",
        "directory",
        "file-system"
    ],
    "authors": [
        {
            "name": "mitsuru793",
            "email": "mitsuru793@gmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit --color=always",
        "analyze": "phpstan analyze ./src"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/stopwatch": "^5.2",
        "illuminate/collections": "^8.18",
        "symfony/finder": "^5.2",
        "nesbot/carbon": "^2.43"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "phpstan/phpstan": "^0.12.65"
    },
    "autoload": {
        "psr-4": {
            "Mitsuru793\\FileConstructor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTest\\": "tests/unit/",
            "TestHelper\\": "tests/helper/"
        }
    }
}
